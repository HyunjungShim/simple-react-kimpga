{
  "name": "react-kimpga",
  "version": "1.0.0",
  "description": "Kimpga React + Express App",
  "main": "server/push-server.js",
  "scripts": {
    "start": "node server/push-server.js",
    "build:client": "cd client && npm install && npm run build",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "socket.io": "^4.8.1",
    "web-push": "^3.6.6",
    "ws": "^8.14.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 